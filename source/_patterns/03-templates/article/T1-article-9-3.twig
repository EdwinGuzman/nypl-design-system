{% set article_vars = 
  {
    'headline': ( headline ),
    'article_hero_image': ( article_hero_image ),
    'breadcrumbs': {
      'link1': ( link1 ),
      'link2': ( link2 ),
      'link3': ( link3 ),
      'link4': ( link4 ),
    },
    catalog_items: {
      header: 'Booklist Header',
      border: true,
      images: true,
      link: 'link',
      intro: 'White, percolator aroma, trifecta, doppio blue mountain eu con panna kopi-luwak café au lait. Pumpkin spice galão breve skinny so sugar cream seasonal White, percolator aroma, trifecta, doppio blue mountain eu con panna',
      items: {
        item_1: {
          'media_type': 'cd',
          'title': ( headline ),
          'year': ( year.long ),
          'file_type': "Computer File",
          'author': {
            'first': ( author.first ),
            'last': ( author.last )
          }
        }
      }
    }
  }
%}

{% extends "@templates/layout.twig" %}

{% block breadcrumbs %}
  {% include "@molecules/navigation/MN-001-breadcrumb.twig" with article_vars %}
{% endblock %}

