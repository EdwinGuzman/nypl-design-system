    {% include "@atoms/typography/AT-001-page-title-h1.twig" %}

    {% include "@atoms/typography/AT-010-body-text.twig" %}

    <h2>{{ headline }}</h2>
    {% include "@organisms/catalog-items/00_catalogs.twig" with article_vars.catalog_items %}

    {% include "@organisms/image-gallery/00_image-gallery.twig" with { header: 'Image Gallery Header' } %}
